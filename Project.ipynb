   "source": [
    "#джойним когортный месяц к общей таблице\n",
    "\n",
    "valid_orders = valid_orders.merge(first_orders[['customer_unique_id','cohort_month']], \n",
    "                                  on='customer_unique_id', how='left')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 176,
   "metadata": {},
   "outputs": [],
   "source": [
    "#считаем разницу в месяцах между датой заказа и датой первого заказа пользователем\n",
    "\n",
    "def month_diff(end, start):\n",
    "    return (end.year - start.year)*12 + (end.month - start.month)\n",
    "\n",
    "valid_orders['months_since_cohort'] = valid_orders.apply(\n",
    "    lambda x: month_diff(x['order_month'], x['cohort_month']), axis=1)\n"
   ]
  },
